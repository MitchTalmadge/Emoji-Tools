<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.web.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mitchtalmadge.emojitools.gui.aptiapi.controllers.UpdateNoticeDialogController">
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
   <top>
      <Label fx:id="headerLabel" text="Emoji Tools Vx.x has been Released!" BorderPane.alignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </top>
   <center>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Label minHeight="-Infinity" text="An update to Emoji Tools has been released. View the change log below to see what's new!" wrapText="true" />
            <Label text="Change Log">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <WebView fx:id="changeLogView" minHeight="200.0" minWidth="400.0" prefHeight="-1.0" prefWidth="-1.0" />
            <GridPane hgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" />
                <ColumnConstraints hgrow="SOMETIMES" percentWidth="50.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="goToDownloadsButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onGoToDownloadsButtonFired" text="Go to Downloads Page" />
                  <Button fx:id="remindMeLaterButton" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onRemindMeLaterButtonFired" text="Remind me Later" GridPane.columnIndex="1" />
               </children>
            </GridPane>
         </children>
      </VBox>
   </center>
</BorderPane>
